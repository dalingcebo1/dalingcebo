# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Stripe Configuration
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# Yoco Configuration (Optional - South African payment provider)
NEXT_PUBLIC_YOCO_PUBLIC_KEY=your_yoco_public_key
YOCO_SECRET_KEY=your_yoco_secret_key
YOCO_WEBHOOK_SECRET=your_yoco_webhook_secret

# Google OAuth (Optional)
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id

# Email Service (Resend)
RESEND_API_KEY=your_resend_api_key
EMAIL_FROM=Dalingcebo Art <orders@dalingcebo.art>

# Application Settings
NEXT_PUBLIC_BASE_URL=https://yourdomain.com
NEXT_PUBLIC_ADMIN_KEY=your_admin_access_key

# Node Environment
NODE_ENV=production
